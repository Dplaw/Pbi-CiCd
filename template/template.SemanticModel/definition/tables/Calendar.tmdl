	partition Calendar = calculated
		mode: import
		source =
				
				VAR FiscalStartMonth = 1
				VAR StartDate = MINX ( ALL ( 'Sales_fct' ), 'Sales_fct'[Month_Year] )
				VAR EndDate   = MAXX ( ALL ( 'Targets Fy25_fct' ), 'Targets Fy25_fct'[FyEnd] )
				
				RETURN
				ADDCOLUMNS (
				    CALENDAR ( StartDate, EndDate ),
				    "Year", YEAR ( [Date] ),
				    "Month No", MONTH ( [Date] ),
				    "Month", FORMAT ( [Date], "MMMM" ),
				    "Year-Month", FORMAT ( [Date], "YYYY-MM" ),
				    "Year-Month Sort", YEAR ( [Date] ) * 100 + MONTH ( [Date] ),
				    "Quarter", "Q" & INT ( ( MONTH ( [Date] ) + 2 ) / 3 ),
				    "Day", DAY ( [Date] ),
				    "DayOfWeek No", WEEKDAY ( [Date], 2 )
				)

	annotation PBI_Id = 416d80e0a2d1409cb193b8360b6d29e4

